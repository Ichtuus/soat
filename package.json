    "@angular/cli": "~12.2.11",
    "@angular/compiler-cli": "~12.2.11",
    "@ngrx/schematics": "^12.5.0",
    "@schematics/angular": "~12.0.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.7.0",
    "json-server": "^0.16.3",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
